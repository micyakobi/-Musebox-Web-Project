@model IEnumerable<Musebox_Web_Project.Models.Branch>


                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.BranchName)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Address)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Latitude)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Longitude)
                            </td>
                            <td class="td-actions">
                                <a asp-action="Edit" asp-route-id="@item.BranchId">Edit</a> |
                                <a asp-action="Details" asp-route-id="@item.BranchId">Details</a> |
                                <a asp-action="Delete" asp-route-id="@item.BranchId">Delete</a>
                            </td>
                        </tr>
                    }
                
